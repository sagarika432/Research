{{#each applications}}
<div class="card card-body mb-2">
   <div class="card card-body">
       <h4>Student Details</h4>
       <p>Name: {{student.name}}</p>
       <p>Email: {{student.email}}</p>
       <p>Department: {{student.department}}</p>
       <p>Year: {{student.year}}</p>
       <p>Github: <a href="https://github.com/{{student.github}}" target="_blank">{{student.name}}'s Github Profile</a> </p>
       <p>UID: {{student.UID}}</p>
       <p>College: {{student.collegeName}}</p>
       <p>Resume: <a href="{{student.resume}}" target="_blank">{{student.name}}'s resume</a></p>
   </div>
   <div class="card card-body">
       <h4>Research Work Details</h4>
       <p>Project: {{project.project}}</p>
       <p>Sponsor: {{project.sponsor}}</p>
       <p>Grants: {{project.grants}}</p>
       <p>Time Of Completion: {{project.timeOfCompletion}}</p>
   </div>
    <a class="btn btn-primary btn-block mb-2" href="/professor/application/approve/{{_id}}">Approve</a>
    <a class="btn btn-danger red btn-block mb-2" href="/professor/application/reject/{{_id}}">Reject</a>
</div>
{{else}}
<p>No Applications Listed :(</p>

{{/each}}